      "expo-router"
    ],
    "name": "OneTaskAi",
    "slug": "OneTaskAi",
    "notification": {
      "icon": "./assets/notification-icon.png",
      "color": "#ffffff",
      "sounds": [
        "./assets/notification-sound.wav"
      ]
    },
    "android": {
      "permissions": [
        "NOTIFICATIONS",
        "NOTIFICATIONS"
      ],
      "package": "com.anonymous.OneTaskAi"
    },
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3340fe2e-d142-4f55-abe3-05aef3b06e50"
      }
    }
  }
}
